/* pages/timeTable/timeList/timeList.wxss */
@import"listdetails/timeList-template.wxss";
page{
  height: 100%;
  background-color: #fff;
}
.List{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.list{
  border-radius: 30rpx;
  background-color: #76C1C6;
  line-height: 25rpx;
  font-size: 36rpx;
  font-weight: bold;
  color:#fff;
}
.title{
  display: flex;
  flex-direction: row;
  align-items: center;
  color: #fff;
  font-weight: bold;
  margin-bottom: 20rpx;
}
.titlebox{
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #76C1C6;
}